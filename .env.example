# Your Sentry DSN (from your Sentry account)
VITE_SENTRY_DSN=

# Your Sentry organization (from your Sentry account)
VITE_SENTRY_ORG=

# Your Sentry project (from your Sentry account)
VITE_SENTRY_PROJECT=

# Your Sentry authentication token (from your Sentry account)
SENTRY_AUTH_TOKEN=
# Database URL for PostgreSQL
DATABASE_URL="postgresql://username:password@localhost:5432/mydb"

### POSTGRESQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=test
POSTGRES_PASSWORD=test
POSTGRES_DB=tanstack
DATABASE_URL="postgresql://test:test@localhost:5432/tanstack?schema=public"

### REDIS
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=test
REDIS_DB=0
REDIS_URL="redis://:${REDIS_PASSWORD}@${REDIS_HOST}:${REDIS_PORT}/${REDIS_DB}"
REDIS_CACHE_URL="redis://test@localhost:6379/0"
REDIS_CACHE_TTL=60
REDIS_QUEUE_URL="redis://test@localhost:6379/0"
REDIS_QUEUE_NAME=tanstack-queue
REDIS_QUEUE_CONCURRENCY=1
REDIS_QUEUE_REDIS_URL="redis://test@localhost:6379/0"
REDIS_QUEUE_REDIS_TLS=false